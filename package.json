{
  "name": "chat-backend-api",
  "version": "1.0.0",
  "description": "Complete chat system backend API with Express.js, TypeScript, MongoDB, Redis, and RabbitMQ",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "seed": "ts-node src/utils/seed.ts",
    "test": "echo \"No tests specified\" && exit 0",
    "test:api": "node test_comprehensive.js",
    "test:websocket": "node test_websocket.js",
    "test:health": "node scripts/health-check.js",
    "deploy": "node scripts/deploy.js deploy",
    "deploy:stop": "node scripts/deploy.js stop",
    "health": "node scripts/deploy.js health",
    "ssl:setup": "node scripts/ssl-setup.js",
    "backup:create": "node scripts/backup.js create",
    "backup:restore": "node scripts/backup.js restore",
    "backup:list": "node scripts/backup.js list",
    "backup:cleanup": "node scripts/backup.js cleanup",
    "backup:schedule": "node scripts/backup.js schedule",
    "monitor": "node scripts/monitoring.js",
    "monitor:dashboard": "node scripts/dashboard.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",    "type-check": "tsc --noEmit",
    "coverage": "nyc npm test",
    "production-guide": "node scripts/final-deployment-guide.js",
    "security-audit": "node scripts/security-audit.js",
    "performance-test": "node scripts/performance-analysis.js",
    "production-ready": "node scripts/production-readiness.js"
  },
  "keywords": [
    "chat",
    "api",
    "express",
    "typescript",
    "mongodb",
    "redis",
    "rabbitmq"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/socket.io": "^3.0.1",
    "amqplib": "^0.10.3",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cluster": "^0.7.7",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "redis": "^4.6.12",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint": "^9.28.0",
    "node-cron": "^4.1.0",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.0.2",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "prom-client": "^15.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
